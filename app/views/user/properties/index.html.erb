<h1>Listing user_properties</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_properties.each do |user_property| %>
      <tr>
        <td><%= link_to 'Show', user_property %></td>
        <td><%= link_to 'Edit', edit_user_property_path(user_property) %></td>
        <td><%= link_to 'Destroy', user_property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

